{
    "manifest_version": 3,
    "name": "ImTools",
    "version": "0.0.5",
    "description": "Youtube Shorts link to Normal video link, Search on namu wiki, Pdf no toolbar viewer",
    "permissions": [
        "contextMenus",
        "storage",
        "tabs"
    ],
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "action": {
        "default_title": "ImTools",
        "default_icon": "icons/32.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}